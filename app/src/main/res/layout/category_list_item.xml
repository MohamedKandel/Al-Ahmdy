<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:cardBackgroundColor="#F8F5F2"
    app:cardCornerRadius="@dimen/_8sdp"
    app:cardElevation="@dimen/_5sdp"
    app:contentPaddingRight="@dimen/_15sdp"
    app:contentPaddingLeft="@dimen/_15sdp"
    app:contentPaddingTop="@dimen/_8sdp"
    app:contentPaddingBottom="@dimen/_8sdp"
    android:layout_marginEnd="@dimen/_6sdp"
    app:strokeWidth="0dp"
    android:layout_marginStart="@dimen/_5sdp"
    android:layout_marginBottom="@dimen/_15sdp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txt_category"
        android:text="@string/duaa"
        android:textColor="@color/btm_bar_color"
        android:fontFamily="@font/poppins_medium"
        android:textSize="@dimen/_11ssp"/>


</com.google.android.material.card.MaterialCardView>